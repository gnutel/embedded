####### Compile

.obj/main.o: main.cpp .ui/mainwnd.h \
		data/common.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.obj/common.o: data/common.cpp data/common.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/common.o data/common.cpp

.ui/mainwnd.h: mainwnd.ui data/common.h
	$(UIC) mainwnd.ui -o .ui/mainwnd.h

.ui/mainwnd.cpp: .ui/mainwnd.h mainwnd.ui mainwnd.ui.h data/common.h
	$(UIC) mainwnd.ui -i mainwnd.h -o .ui/mainwnd.cpp

.obj/mainwnd.o: .ui/mainwnd.cpp mainwnd.ui.h \
		.ui/mainwnd.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/mainwnd.o .ui/mainwnd.cpp

.obj/moc_mainwnd.o: .moc/moc_mainwnd.cpp  .ui/mainwnd.h data/common.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_mainwnd.o .moc/moc_mainwnd.cpp

.moc/moc_mainwnd.cpp: $(MOC) .ui/mainwnd.h
	$(MOC) .ui/mainwnd.h -o .moc/moc_mainwnd.cpp

