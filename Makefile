	@$(QMAKE) -o Makefile access_control.pro

dist: 
	@mkdir -p .obj/access_control && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/access_control/ && $(COPY_FILE) --parents mainwnd.ui.h .obj/access_control/ && ( cd `dirname .obj/access_control` && $(TAR) access_control.tar access_control && $(GZIP) access_control.tar ) && $(MOVE) `dirname .obj/access_control`/access_control.tar.gz . && $(DEL_FILE) -r .obj/access_control

mocclean:
